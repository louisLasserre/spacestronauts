<div id="<%= dom_id rocket %>">
  <p>
    <strong>Name:</strong>
    <%= rocket.name %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= link_to rocket.company, rocket.company %>
  </p>

  <ul><p><strong>Peut aller sur :</p></strong>
    <% rocket.destinations.each do |destination| %>
      <li><%= link_to destination, destination %></li>
    <% end %>
  </ul>

  <p>
    <strong>Image:</strong>
    <!-- image_tag rocket.image-->
    <%= image_tag rocket.rocketImg %>
    <br>
  </p>

</div>
